{
  "name": "stack-attack-bot",
  "version": "1.0.0",
  "private": true,
  "description": "Stack Attack helper and lander",
  "author": "E-Liang Tan <eliang@eliangtan.com>",
  "license": "ISC",
  "repository": "https://github.com/sttack/stack-attack-bot.git",
  "homepage": "https://github.com/sttack/stack-attack-bot",
  "bugs": "https://github.com/sttack/stack-attack-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc --incremental -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./dist/src/index.js",
    "lint": "eslint **/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch --notify --notifyMode=change"
  },
  "dependencies": {
    "bunyan": "^1.8.14",
    "github-rebase": "^1.1.0",
    "nullthrows": "^1.1.1",
    "probot": "^9.13.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "codecov": "^3.7.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.2",
    "jest-junit": "^11.1.0",
    "msw": "^0.20.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "smee-client": "^1.1.0",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">= 12.4.0"
  }
}
